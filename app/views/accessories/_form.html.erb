<%= form_for([@reserve, @garrison, @accessory]) do |f| %>
  <% if @accessory.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@accessory.errors.count, "error") %> prohibited this accessory from being saved:</h2>

      <ul>
      <% @accessory.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<div class="new-form">
  <div class="field">
    <%= f.label :Descrição %><br>
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :Quantidade %><br>
    <%= f.number_field :amount %>
  </div>
  <div class="actions">
      <%= f.submit "OK", :type => "submit" , :class => "btn btn-primary" %>
  </div>
<% end %>
</div>