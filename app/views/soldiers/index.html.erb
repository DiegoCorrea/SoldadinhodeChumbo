<p id="notice"><%= notice %></p>

<h1>Listing Soldiers</h1>

<table>
  <thead>
    <tr>
      <th>Function</th>
      <th>Warname</th>
      <th>Reserve</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @soldiers.each do |soldier| %>
      <tr>
        <td><%= soldier.function %></td>
        <td><%= soldier.warName %></td>
        <td><%= soldier.reserve %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>